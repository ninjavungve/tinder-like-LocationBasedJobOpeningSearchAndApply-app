{
"parser": "babel-eslint",
"extends": ["airbnb", "plugin:react-native/all"],
"plugins": [
        "react-native"
    ],
"rules": {
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0,
    "max-len": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-indent": 0,
    "react/jsx-indent-props": 0,
    "react/prop-types": 0,
    "class-methods-use-this": 0,
    "camelcase": 0,
    "radix": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    }
}
